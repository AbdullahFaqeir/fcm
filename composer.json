{
    "name": "abdullahfaqeir/fcm",
    "description": "FCM (Firebase Cloud Messaging) Notifications Driver for Laravel",
    "homepage": "https://github.com/AbdullahFaqeir/fcm",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Bautista",
            "email": "chris.bautista@coreproc.ph",
            "homepage": "https://coreproc.com",
            "role": "Developer"
        },
        {
            "name": "Abdullah Al-Faqeir",
            "email": "abdullah@devloops.net",
            "homepage": "https://devloops.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/notifications": "~8.0 || ~9.0",
        "illuminate/support": "~8.0 || ~9.0",
        "kreait/laravel-firebase": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Fcm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Fcm\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Fcm\\FcmServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
